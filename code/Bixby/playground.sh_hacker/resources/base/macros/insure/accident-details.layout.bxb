layout-macro-def (accident-details){
	params{
		param (accidentset){
			type (insureSubSet)
			min (Optional)
			max (Many)
		}
	}
	content{
		section{
			content{
				for-each (accidentset){
					as (accident) {
						title-card {
							title-area {
								halign (Start)
								slot1 {
									text {
										value {
											template ("#{value(accident.name)}")
										}
									}
								}
								slot2 {
									paragraph {
										value {
											template ("#{value(accident.fee)}")
										}
									}
								}
								slot3 {
									paragraph {
										value {
											template ("#{value(accident.description)}")
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}
}